{
  "expo": {
    "name": "Grey Matter",
    "slug": "grey-matter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/brainicon.png",
    "splash": {
      "image": "./src/images/brain.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "vue"
      ]
    },
    "android": {
      "package": "com.anthonyd25.greymatter",
      "versionCode": 2
    }
  }
}
